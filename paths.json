{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "baseUrl": "./",
        "paths": {
            "@page/*": ["src/pages/*"],
            "@types/*": ["src/types/*"],
            "@features/*": ["src/features/*"],
            "@assets/*": ["src/assets/*"],
            "@reduxStore/*": ["src/app/*"],
            "@sharedComponent/*": ["src/shared/components/*"],
            "@sharedFunction/*": ["src/shared/functions/*"],
            "@customHook/*": ["src/shared/hooks/*"]
        },
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx"
    }
}

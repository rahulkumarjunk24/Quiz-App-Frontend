{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "baseUrl": "./",
        "paths": {
            "@Page/*": ["src/pages/*"],
            "@Type/*": ["src/types/*"],
            "@Feature/*": ["src/features/*"],
            "@Asset/*": ["src/assets/*"],
            "@ReduxStore/*": ["src/app/*"],
            "@SharedComponent/*": ["src/shared/components/*"],
            "@SharedFunction/*": ["src/shared/functions/*"],
            "@CustomHook/*": ["src/shared/hooks/*"]
        },
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx"
    },
    "include": ["src"]
}
